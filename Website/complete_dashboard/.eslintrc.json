{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off", // Ignore unused variables
    "@typescript-eslint/no-explicit-any": "off", // Allow 'any' type
    "jsx-a11y/alt-text": "off", // Disable missing alt text warnings
    "react/no-unescaped-entities": "off", // Ignore unescaped characters like quotes
    "@next/next/no-img-element": "off" // Allow using <img> instead of <Image>
  }
}
